{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testMatch": ["<rootDir>/backend/test/**/*.e2e-spec.ts"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e",
  "testTimeout": 30000,
  "globalSetup": "<rootDir>/backend/test/setup-e2e.ts",
  "globalTeardown": "<rootDir>/backend/test/teardown-e2e.ts"
}
